{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-mixed-operators": 0,
    "react/require-extension": 0,
    "arrow-body-style": 0,
    "no-confusing-arrow": 0,
    "import/prefer-default-export": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "no-else-return": 0,
    "no-process-env": 2,
    "quotes": [1, "double"],
    "no-console": [2, { "allow": ["error"] }]
  }
}